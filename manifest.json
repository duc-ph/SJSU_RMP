{
    "manifest_version": 3,
    "name": "SJSU RateMyProfessor",
    "version": "1.0",
    "description": "Display the rating of every professor in the MySJSU class search page.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },
    "permissions": [
      "storage"
      ],
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [ 
            "https://cmsweb.cms.sjsu.edu/*"
          ]
        }
      ]
  }